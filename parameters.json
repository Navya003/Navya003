{
  "name" : "random",
  "method" : "random",
  "metric": {
    "name": "eval/f1",
    "goal": "maximize"
  },
  "parameters" : {
    "epochs" : {
      "values" : [1, 2]
    },
    "batch_size": {
        "values": [8, 16]
        },
    "learning_rate" :{
      "distribution": "log_uniform_values",
      "min": 0.01,
      "max": 0.1
      },
    "weight_decay": {
        "values": [0.1, 0.2]
      }
  } 
}
  
